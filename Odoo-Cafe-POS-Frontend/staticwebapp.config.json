{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*",
            "*.js",
            "*.css",
            "*.png",
            "*.jpg",
            "*.svg",
            "*.ico",
            "*.woff",
            "*.woff2"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "statusCode": 404
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    }
}